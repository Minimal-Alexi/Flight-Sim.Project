def check_end_goal(current_location):
    if current_location == "LA":
        return True
    else:
        return False

def victory_screen():
    print("Congratulations! You've successfully reached Los Angeles International Airport!")
    print("Thank you for flying!")

# If current location 
current_location = "LA"

if check_end_goal(current_location):
    victory_screen()
else:
    print("Keep flying, you're getting closer to LA! and Visit HOLLYWOOD!")
